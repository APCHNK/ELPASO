<section id="breadcrumbs">
    <div class="container">
        <div class="flex">
            <a href="/">Home</a>
            <span class="sep"></span>
            <a href="/blogs/news">Blog</a>
            <span class="sep"></span>
            <span>{{ article.title }}</span>
        </div>
    </div>
</section>

<article>
    <div class="container flex">
        <div class="content">
            <h1>{{ article.title }}</h1>
            <div class="author">
                <div class="data flex">
                    <img src="{{ 'user.png' | asset_url}}" alt="title">
                    <div class="name">{{ article.author }} &nbsp;&nbsp;|&nbsp;&nbsp; <span>{{ article.published_at | date: '%B %d, %Y' }}</span></div>
                </div>
                <div class="excerpt">{{ article.excerpt }}</div>
            </div>
            <div class="thumbnail">
                <img src="{{ 'blog_post.jpg' | asset_url}}" alt="title">
            </div>
            {{ article.content }}
             </div>
