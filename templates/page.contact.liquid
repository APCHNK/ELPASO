<section id="promo__page" class="promo__contacts">
		<div class="container">
			<h1>Contact El Paso Artificial Grass Supply</h1>
			<div class="bt__note">Have questions or ready to start your project? Reach out to our friendly team today for expert guidance and personalized assistance.</div>
		</div>
	</section>

	<section id="contacts">
		<div class="container flex">
			<div class="data col">
				<div class="inn">
					<div class="title">El Paso Artificial Grass Supply</div>
					<p class="phone"><a href="tel:915201421">915 - 201 - 421</a></p>
					<p class="address">11220 Rojas Dr Suite A2, El Paso, TX 79935</p>
				</div>
				<div class="map">
					<iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d216992.81050118041!2d-106.5893233074461!3d31.811029076738627!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x86e743595d83472d%3A0xadd13e3601dd0110!2sEl%20Paso%20Artificial%20Grass%20Supply!5e0!3m2!1sen!2sus!4v1719530413152!5m2!1sen!2sus" width="600" height="450" style="border:0;" allowfullscreen="" loading="lazy" referrerpolicy="no-referrer-when-downgrade"></iframe>
				</div>
			</div>
			<div class="form col">
				{% form 'contact' %}
					{{ form.errors | default_errors }}
					<div class="flex">
						<input type="text" name="contact[first_name]" id="first-name" placeholder="Name">
						<input type="email" name="contact[email]" id="email" required placeholder="Email">
					</div>
					<div class="flex">
						<input type="tel" name="contact[phone]" id="phone" placeholder="Phone">
						<select name="">
							<option value="">How Can We Help?</option>
							<option value="">How Can We Help?</option>
							<option value="">How Can We Help?</option>
							<option value="">How Can We Help?</option>
						</select>
						<input type="hidden" name="contact[option]" id="option" value="1">
					</div>
					<textarea type="text" name="contact[body]"  id="message" placeholder="Message"></textarea>
					<button class="btn btn-orange">Send Message</button>
				{% endform %}
			</div>
		</div>
	</section>
	<script>
		document.addEventListener('DOMContentLoaded', function() {
			const selectElement = document.querySelector('select');
			const inputElement = document.querySelector('#option');

			selectElement.addEventListener('change', function() {
				const selectedOption = selectElement.options[selectElement.selectedIndex];
				inputElement.value = selectedOption.textContent;
			});
		});
	</script>
