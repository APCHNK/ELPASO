<section id="breadcrumbs">
    <div class="container">
        <div class="flex">
            <a href="#">Home</a>
            <span class="sep"></span>
            <span>Cart</span>
        </div>
    </div>
</section>

<section id="cart__page">
    {% form 'cart'
        , cart %}
    <div class="container">
        <div class="top flex">
            <div class="bt">Cart</div>
            <div class="actions flex">
                <a href="javascript:history.back()" class="btn btn-orange-tr">Continue Shopping</a>
                <button type="submit" name="checkout" class="btn btn-orange">Proceed to Checkout</button>
            </div>
        </div>

        <div class="list__cart">
            <div class="item__head flex">
                <div class="col">Product</div>
                <div class="col">Price</div>
                <div class="col">Quantity</div>
                <div class="col">Subtotal</div>
            </div>
            {% for product in cart.items %}
                <div class="item flex cart-item" style="user-select: none; " data-key="{{ product.key }}">
                    <div class="col col__product">
                        <div class="image"><img src="{{ product.image | image_url }}" alt="title"></div>
                        <div class="title">{{ product.title }}</div>
                    </div>
                    <div class="col col__price price">{{ product.price | money }}</div>
                    <div class="col">
                        <div class="count flex">
                            <div class="minus down"><svg width="17" height="17" viewBox="0 0 17 17" fill="none" xmlns="http://www.w3.org/2000/svg"><path d="M14.082 8.5C14.082 8.63261 14.0294 8.75979 13.9356 8.85355C13.8418 8.94732 13.7146 9 13.582 9H2.58203C2.44942 9 2.32225 8.94732 2.22848 8.85355C2.13471 8.75979 2.08203 8.63261 2.08203 8.5C2.08203 8.36739 2.13471 8.24021 2.22848 8.14645C2.32225 8.05268 2.44942 8 2.58203 8H13.582C13.7146 8 13.8418 8.05268 13.9356 8.14645C14.0294 8.24021 14.082 8.36739 14.082 8.5Z" fill="#121312"/></svg></div>
                            <input type="text" class="quantity-input" name="" value="{{ product.quantity }}">
                            <div class="plus up"><svg width="17" height="17" viewBox="0 0 17 17" fill="none" xmlns="http://www.w3.org/2000/svg"><path d="M14.082 8.5C14.082 8.63261 14.0294 8.75979 13.9356 8.85355C13.8418 8.94732 13.7146 9 13.582 9H8.58203V14C8.58203 14.1326 8.52935 14.2598 8.43559 14.3536C8.34182 14.4473 8.21464 14.5 8.08203 14.5C7.94942 14.5 7.82225 14.4473 7.72848 14.3536C7.63471 14.2598 7.58203 14.1326 7.58203 14V9H2.58203C2.44942 9 2.32225 8.94732 2.22848 8.85355C2.13471 8.75979 2.08203 8.63261 2.08203 8.5C2.08203 8.36739 2.13471 8.24021 2.22848 8.14645C2.32225 8.05268 2.44942 8 2.58203 8H7.58203V3C7.58203 2.86739 7.63471 2.74021 7.72848 2.64645C7.82225 2.55268 7.94942 2.5 8.08203 2.5C8.21464 2.5 8.34182 2.55268 8.43559 2.64645C8.52935 2.74021 8.58203 2.86739 8.58203 3V8H13.582C13.7146 8 13.8418 8.05268 13.9356 8.14645C14.0294 8.24021 14.082 8.36739 14.082 8.5Z" fill="#121312"/></svg></div>
                        </div>
                    </div>
                    <div class="col col__price total">{{ product.line_price | money }}</div>
                    <div class="remove">
                        <a href="#"><svg width="33" height="33" viewBox="0 0 33 33" fill="none" xmlns="http://www.w3.org/2000/svg"><path d="M20.7895 13.2075L17.4958 16.5L20.7895 19.7925C20.8824 19.8854 20.9561 19.9957 21.0064 20.1171C21.0567 20.2385 21.0826 20.3686 21.0826 20.5C21.0826 20.6314 21.0567 20.7615 21.0064 20.8829C20.9561 21.0043 20.8824 21.1146 20.7895 21.2075C20.6966 21.3004 20.5863 21.3741 20.4649 21.4244C20.3435 21.4747 20.2134 21.5006 20.082 21.5006C19.9506 21.5006 19.8205 21.4747 19.6991 21.4244C19.5777 21.3741 19.4674 21.3004 19.3745 21.2075L16.082 17.9137L12.7895 21.2075C12.6966 21.3004 12.5863 21.3741 12.4649 21.4244C12.3435 21.4747 12.2134 21.5006 12.082 21.5006C11.9506 21.5006 11.8205 21.4747 11.6991 21.4244C11.5777 21.3741 11.4674 21.3004 11.3745 21.2075C11.2816 21.1146 11.2079 21.0043 11.1576 20.8829C11.1074 20.7615 11.0815 20.6314 11.0815 20.5C11.0815 20.3686 11.1074 20.2385 11.1576 20.1171C11.2079 19.9957 11.2816 19.8854 11.3745 19.7925L14.6683 16.5L11.3745 13.2075C11.1869 13.0199 11.0815 12.7654 11.0815 12.5C11.0815 12.2346 11.1869 11.9801 11.3745 11.7925C11.5622 11.6049 11.8167 11.4994 12.082 11.4994C12.3474 11.4994 12.6019 11.6049 12.7895 11.7925L16.082 15.0863L19.3745 11.7925C19.4674 11.6996 19.5777 11.6259 19.6991 11.5756C19.8205 11.5253 19.9506 11.4994 20.082 11.4994C20.2134 11.4994 20.3435 11.5253 20.4649 11.5756C20.5863 11.6259 20.6966 11.6996 20.7895 11.7925C20.8824 11.8854 20.9561 11.9957 21.0064 12.1171C21.0567 12.2385 21.0826 12.3686 21.0826 12.5C21.0826 12.6314 21.0567 12.7615 21.0064 12.8829C20.9561 13.0043 20.8824 13.1146 20.7895 13.2075ZM29.082 16.5C29.082 19.0712 28.3196 21.5846 26.8911 23.7224C25.4627 25.8603 23.4324 27.5265 21.0569 28.5104C18.6815 29.4944 16.0676 29.7518 13.5459 29.2502C11.0241 28.7486 8.70773 27.5105 6.88965 25.6924C5.07157 23.8743 3.83344 21.5579 3.33183 19.0362C2.83022 16.5144 3.08766 13.9006 4.0716 11.5251C5.05554 9.14968 6.72178 7.11935 8.85962 5.6909C10.9975 4.26244 13.5109 3.5 16.082 3.5C19.5287 3.50364 22.8332 4.87445 25.2704 7.31163C27.7076 9.74882 29.0784 13.0533 29.082 16.5ZM27.082 16.5C27.082 14.3244 26.4369 12.1977 25.2282 10.3887C24.0195 8.57979 22.3015 7.16989 20.2916 6.33733C18.2816 5.50476 16.0698 5.28692 13.936 5.71136C11.8023 6.1358 9.84224 7.18345 8.30386 8.72183C6.76548 10.2602 5.71784 12.2202 5.2934 14.354C4.86896 16.4878 5.0868 18.6995 5.91936 20.7095C6.75193 22.7195 8.16182 24.4375 9.97076 25.6462C11.7797 26.8549 13.9064 27.5 16.082 27.5C18.9984 27.4967 21.7944 26.3367 23.8566 24.2745C25.9187 22.2123 27.0787 19.4164 27.082 16.5Z" fill="#BABABA"/></svg></a>
                    </div>
                </div>
            {% endfor %}
        </div>

        <div class="cart__actions flex">
            <div class="coupon">
                <div class="info">
                    <div class="title">Have a coupon?</div>
                    <div class="flex">
                        <input type="text" name="" placeholder="Enter coupon code">
                        <button class="btn">Apply</button>
                    </div>
                </div>

                <div class="product__delivery">
                    <div class="flex">
                        <div class="item">
                            <img src="{{ 'delivery1.svg' | asset_url}} " alt="title">
                            <span>Fast Local Delivery</span>
                        </div>
                        <div class="item">
                            <img src="{{ 'delivery2.svg' | asset_url}} " alt="title">
                            <span>Premium Quality</span>
                        </div>
                        <div class="item">
                            <img src="{{ 'delivery3.svg' | asset_url}} " alt="title">
                            <span>Affordable Price</span>
                        </div>
                    </div>
                </div>
            </div>

            <div class="cart__total">
                <div class="title">Cart Total</div>
                <div class="row flex">
                    <span>Subtotal</span> <strong class="subtotal-field">{{ cart.total_price |  money}}</strong>
                </div>
                <div class="row flex">
                    <span>Shipping</span> <strong>Free Shipping</strong>
                </div>
                <div class="row flex">
                    <span>Total</span> <strong class="total subtotal-field">{{ cart.total_price |  money}}</strong>
                </div>

                <button type="submit" name="checkout" class="btn btn-orange">Proceed to Checkout</button>

                <div class="product__pay flex">
                    <img src="{{ 'pay1.svg' | asset_url}} " alt="title">
                    <img src="{{ 'pay2.svg' | asset_url}} " alt="title">
                    <img src="{{ 'pay3.svg' | asset_url}} " alt="title">
                    <img src="{{ 'pay4.svg' | asset_url}} " alt="title">
                    <img src="{{ 'pay5.svg' | asset_url}} " alt="title">
                    <img src="{{ 'pay6.svg' | asset_url}} " alt="title">
                    <img src="{{ 'pay7.svg' | asset_url}} " alt="title">
                </div>
            </div>
        </div>
    </div>
    {% endform %}
</section>
