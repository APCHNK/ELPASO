html{overflow-x: hidden;}



:root{
--apple-green: #7FBE24;
--dark-spring-green: #00783B;
--tomato: #cc4c05;
--white: #fff;
--night: #121312;
--orange: #FF5B00;
--british-racing-green: #013E1F;
}

:root{
--font-family: "Montserrat", sans-serif;
--second-family: "Sora", sans-serif;
}



body{
font-family: "Montserrat", sans-serif;
background: #fff;
font-size: 16px;
line-height: normal;
color: var(--night);
min-width: 320px;
font-weight: normal;
overflow-x: hidden;
position: relative;
}


.container,.container__inn{max-width: 1150px; width: 100%; margin: 0 auto; box-sizing: border-box;}




header{position: absolute; top: 0; left: 0; z-index: 300; width: 100%; transition: all 0.15s; padding: 0 0 30px}
header .container{max-width: 1480px}
header .flex{align-items: center;}
header .top{background: #00783B; color: #fff; font-weight: 600; padding: 16px 0; margin-bottom: 30px;}
header .delivery{padding: 0 0 0 32px; background: url({{ 'delivery.svg' | asset_url}} ) 0 50% no-repeat; margin-right: 16px}
header .money{padding: 0 0 0 32px; background: url({{ 'money.svg' | asset_url}} ) 0 50% no-repeat;}
header .address{margin-left: 14px; padding-left: 15px; background: url({{ 'line.svg' | asset_url}} ) 0 50% no-repeat;}
header .phone{font-size: 18px;}
header .phone a{display: inline-block; color: #fff; line-height: 28px; padding: 0 0 0 34px; margin-left: 8px; background: url({{ 'phone.svg' | asset_url}} ) 0 50% no-repeat;}
header .account{padding-left: 24px; margin-left: 24px; background: url({{ 'line.svg' | asset_url}} ) 0 50% no-repeat;}
header .account a{display: block; background: url({{ 'account.svg' | asset_url}} ) 50% 50% no-repeat; width: 24px; height: 24px}
header .cart{margin-left: 14px}
header .cart a{position: relative; display: block; width: 24px; height: 24px; background: url({{ 'cart.svg' | asset_url}} ) 50% 50% no-repeat;}
header .cart a span{display: flex; align-items: center; justify-content: center; color: #fff; width: 13px; height: 13px; background: var(--orange); border-radius: 50%; font-weight: 600; font-size: 10px; position: absolute; top: 0; right: -5px;}
header nav li{display: inline-block; margin-right: 40px; position: relative;}
header nav li a{display: block; position: relative; color: var(--night); line-height: 26px; font-weight: 500;}
header nav li.has-children{padding-right: 28px;}
header nav li.has-children:after{position: absolute; top: calc(50% - 12px); right: 0; width: 24px; height: 24px; background: url({{ 'arr.svg' | asset_url}} ) 50% 50% no-repeat; transition: all 0.3s; content: ''}
header nav li.has-children:hover:after{transform: rotate(180deg);}
header nav li .sub-menu{position: absolute; display: none; top: 100%; left: 25px; z-index: 299; padding: 45px 0 30px; width: 275px; box-sizing: border-box;}
header nav li .sub-menu:before{bottom: calc(100% - 10px); z-index: 10; left: 65px; border: solid transparent; content: ""; height: 0; width: 0; position: absolute; pointer-events: none; border-color: rgba(255, 255, 255, 0); border-bottom-color: #ffffff; border-width: 7px; margin-left: -7px;}
header nav li .sub-menu:after{position: absolute; z-index: 2; box-shadow: 0px 8px 30px rgba(0, 0, 0, 0.3); box-sizing: border-box; top: 10px; left: 0; width: 100%; height: calc(100% - 10px); background: #fff; border-radius: 20px; transition: all 0.15s; content: ''}
header nav li .sub-menu li{position: static; display: block; z-index: 3; margin: 4px 0; font-family: var(--second-family);}
header nav li .sub-menu li a{display: flex; position: relative; padding: 0 30px; z-index: 3; align-items: center; justify-content: flex-start; font-size: 16px; line-height: 26px;}
header nav li .sub-menu li a svg{margin-right: 5px}
header nav li .sub-menu li:hover a{color: var(--orange)}
header nav li .sub-menu .sub-menu{left: calc(100% - 20px); top: 0; width: 290px; padding: 45px 20px 20px; min-height: 100%; box-sizing: border-box;}
header nav li .sub-menu .sub-menu:before{display: none;}
header nav li .sub-menu .sub-menu:after{background: #F2F2F2; top: 10px; border-radius: 0 20px 20px 0; box-shadow: none; height: calc(100% - 10px); bottom: 0;  box-sizing: border-box;}
header nav li .sub-menu .sub-menu li a{padding: 0; color: var(--night)}
header nav li .sub-menu .sub-menu li:hover > a{color: var(--orange)}
header nav li .sub-menu .sub-menu:after{border-radius: 0 20px 20px 0!important; box-shadow: 0px 8px 30px rgba(0, 0, 0, 0.3); z-index: 1}
header nav li:hover > .sub-menu{display: block;}
header nav li.item-blog .sub-menu{width: 120px; left: 2px; padding: 30px 0 15px}
header nav li.item-blog .sub-menu li a{padding: 0 25px}
header .pull{display: none;}





#promo{background: url({{ 'bg_promo1.jpg' | asset_url}} ) 50% 0 no-repeat; background-size: cover; padding: 225px 0 400px; position: relative;}
#promo.inner{padding-bottom: 85px}
#promo .text{max-width: 545px;}
#promo h1{font-size: 45px; line-height: 50px; font-weight: 700; font-family: var(--second-family); max-width: 610px; margin-bottom: 15px}
#promo h1 strong{display: block; font-size: 24px; font-weight: 600; line-height: 30px; color: #00783B; margin-bottom: 8px;}
#promo .text p{margin-bottom: 30px; line-height: 26px; font-weight: 500;}
#promo .actions{justify-content: flex-start; align-items: center; margin-bottom: 60px;}
#promo .actions .btn-orange{padding: 17px 60px; font-size: 18px; margin-right: 24px}
#promo .advantages{text-align: center; align-items: center;}
#promo .advantages .item{line-height: 16px; font-weight: 500; background: url({{ 'linea.svg' | asset_url}} ) 100% 50% no-repeat; padding-right: 16px}
#promo .advantages .item:last-child{background: transparent;}
#promo .advantages .item .title{display: flex; font-size: 20px; align-items: center; justify-content: center; font-family: var(--second-family); line-height: 24px; margin-bottom: 8px; font-weight: 600;}
#promo .advantages .item .title img{margin-left: 8px}
#promo.promo__about{padding-bottom: 80px; background: url({{ 'bg_about.png' | asset_url}} ) 50% 0 no-repeat; background-size: cover;}
#promo.promo__about h1{margin-bottom: 40px}





#help{position: relative; margin-top: -330px}
#help .container{padding: 40px 137px 1px; max-width: 1480px; background: #fff; border-radius: 30px 30px 0 0; box-shadow: 0px -127px 51px rgba(0, 24, 12, 0.01), 0px -72px 43px rgba(0, 24, 12, 0.02), 0px -32px 32px rgba(0, 24, 12, 0.02), 0px -8px 18px rgba(0, 24, 12, 0.02);}
#help .bt{margin-bottom: 40px; font-size: 24px; line-height: 30px; text-align: center;}
#help .list__help{filter: drop-shadow(0px 4px 20px rgba(0, 0, 0, 0.08));}
#help .list__help .item{border: 1.5px solid rgba(18, 19, 18, 0.1); background: #fff; border-radius: 20px; overflow: hidden; box-sizing: border-box; width: calc(25% - 5px); padding: 30px; position: relative; transition: all 0.3s;}
#help .list__help .item:after{position: absolute; top: 0; left: 0; width: 100%; height: 100%; content: ''; background: linear-gradient(331.21deg, #00783B 49.02%, #7FBE24 99.94%); opacity: 0; transition: all 0.3s;}
#help .list__help .item:hover:after{opacity: 1}
#help .list__help .item:hover{border-color: #00783B; color: #fff;}
#help .list__help .item:hover a{color: #fff; border-color: #fff}
